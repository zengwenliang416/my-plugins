{
  "timestamp": "${timestamp}",
  "version": "1.0",
  "source_stack": {
    "frontend": ["${frontend_framework}", "${ui_library}"],
    "backend": ["${backend_language}", "${backend_framework}"],
    "database": ["${database_type}"],
    "build": ["${build_tool}"],
    "infrastructure": ["${infra_type}"]
  },
  "target_stack": {
    "frontend": ["${target_frontend_framework}", "${target_ui_library}"],
    "backend": ["${target_backend_language}", "${target_backend_framework}"],
    "database": ["${target_database_type}"],
    "build": ["${target_build_tool}"],
    "infrastructure": ["${target_infra_type}"]
  },
  "analysis": {
    "architecture_pattern": "${pattern}",
    "coupling_score": 0,
    "technical_debt_score": 0,
    "test_coverage": 0,
    "loc": 0,
    "file_count": 0,
    "module_count": 0
  },
  "strategy": {
    "name": "${strategy_name}",
    "rationale": "${strategy_rationale}",
    "estimated_duration": "${duration}",
    "confidence": "${confidence}"
  },
  "phases": [
    {
      "id": 1,
      "name": "${phase_1_name}",
      "description": "${phase_1_description}",
      "duration": "${phase_1_duration}",
      "risk": "${phase_1_risk}",
      "dependencies": [],
      "deliverables": ["${deliverable_1}", "${deliverable_2}"],
      "success_criteria": ["${criteria_1}", "${criteria_2}"]
    },
    {
      "id": 2,
      "name": "${phase_2_name}",
      "description": "${phase_2_description}",
      "duration": "${phase_2_duration}",
      "risk": "${phase_2_risk}",
      "dependencies": [1],
      "deliverables": [],
      "success_criteria": []
    }
  ],
  "seams": [
    {
      "id": "SEAM-001",
      "name": "${seam_name}",
      "type": "${seam_type}",
      "priority": 1,
      "risk": "${seam_risk}",
      "dependencies": [],
      "effort": "${seam_effort}",
      "files": ["${file_1}", "${file_2}"],
      "migration_approach": "${approach}"
    }
  ],
  "risks": [
    {
      "id": "RISK-001",
      "name": "${risk_name}",
      "category": "${risk_category}",
      "impact": "${impact}",
      "likelihood": "${likelihood}",
      "level": "${level}",
      "mitigation": "${mitigation}",
      "contingency": "${contingency}"
    }
  ],
  "tech_debt": {
    "total_score": 0,
    "categories": {
      "code_quality": 0,
      "architecture": 0,
      "test_coverage": 0,
      "dependencies": 0,
      "documentation": 0
    },
    "critical_items": [
      {
        "type": "${debt_type}",
        "location": "${debt_location}",
        "impact": "${debt_impact}",
        "fix_priority": "${fix_priority}"
      }
    ]
  },
  "recommendations": {
    "immediate": ["${immediate_1}", "${immediate_2}"],
    "short_term": ["${short_term_1}", "${short_term_2}"],
    "long_term": ["${long_term_1}", "${long_term_2}"]
  },
  "metrics": {
    "target_migration_percentage": 100,
    "current_migration_percentage": 0,
    "legacy_loc_remaining": 0,
    "new_service_count": 0
  }
}
