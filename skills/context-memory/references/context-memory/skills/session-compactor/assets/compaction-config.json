{
  "strategies": {
    "aggressive": {
      "keep_decisions": true,
      "keep_code_changes": true,
      "keep_errors": true,
      "keep_user_feedback": true,
      "remove_exploration": true,
      "remove_failed_attempts": true,
      "target_ratio": 0.2
    },
    "balanced": {
      "keep_decisions": true,
      "keep_code_changes": true,
      "keep_errors": true,
      "keep_user_feedback": true,
      "remove_exploration": false,
      "remove_failed_attempts": true,
      "target_ratio": 0.4
    },
    "conservative": {
      "keep_decisions": true,
      "keep_code_changes": true,
      "keep_errors": true,
      "keep_user_feedback": true,
      "remove_exploration": false,
      "remove_failed_attempts": false,
      "target_ratio": 0.6
    }
  },
  "sections": {
    "must_keep": [
      "user_request",
      "final_decision",
      "code_changes",
      "error_resolution"
    ],
    "may_remove": [
      "exploration",
      "failed_attempts",
      "verbose_output",
      "intermediate_states"
    ]
  },
  "output": {
    "format": "markdown",
    "include_timestamp": true,
    "include_metadata": true
  }
}
